{
  "name": "vite-plugin-env-typed",
  "description": "A vite plugin to generate import_meta.d.ts file for env typed.",
  "keywords": [
    "vite",
    "vite-plugin",
    "env",
    "import",
    "meta",
    "import.meta",
    "typed",
    "env-typed",
    "typescript",
    "type"
  ],
  "version": "0.0.2",
  "scripts": {
    "build": "rimraf types && rimraf dist && tsdown && rolldown -c rolldown.config.ts && cp -r src/tmpl dist/es/tmpl && cp -r src/tmpl dist/cjs/tmpl",
    "ci:publish": "pnpm changeset publish -r",
    "test": "vitest",
    "lint-staged": "lint-staged"
  },
  "type": "module",
  "files": [
    "dist",
    "types/**/*.d.ts",
    "!dist/**/*.map",
    "!types/**/*.map",
    "README.md",
    "LICENSE",
    "README.en.md"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/es/index.mjs",
  "types": "./types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/JsonLee12138/vite-plugin-env-typed"
  },
  "bugs": {
    "url": "https://github.com/JsonLee12138/vite-plugin-env-typed/issues",
    "email": "lijunsong2@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "handlebars": "^4.7.8"
  },
  "peerDependencies": {
    "vite": "^6.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/node": "^22.13.14",
    "cz-git": "^1.11.1",
    "husky": "^9.1.7",
    "lint-staged": "^13.2.0",
    "oxlint": "^0.16.3",
    "oxlint-config": "workspace:^",
    "rimraf": "^6.0.1",
    "rolldown": "1.0.0-beta.9",
    "rolldown-plugin-dts": "^0.13.6",
    "tsdown": "^0.12.4",
    "typescript": "^5.7.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
