#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Bone/Documents/RAGFLOW/ai-tongtong/node_modules/.pnpm/@antfu+eslint-config@4.19.0_@vue+compiler-sfc@3.5.22_eslint@9.36.0_jiti@2.6.0__typescript@5.8.3/node_modules/@antfu/eslint-config/bin/node_modules:/Users/Bone/Documents/RAGFLOW/ai-tongtong/node_modules/.pnpm/@antfu+eslint-config@4.19.0_@vue+compiler-sfc@3.5.22_eslint@9.36.0_jiti@2.6.0__typescript@5.8.3/node_modules/@antfu/eslint-config/node_modules:/Users/Bone/Documents/RAGFLOW/ai-tongtong/node_modules/.pnpm/@antfu+eslint-config@4.19.0_@vue+compiler-sfc@3.5.22_eslint@9.36.0_jiti@2.6.0__typescript@5.8.3/node_modules/@antfu/node_modules:/Users/Bone/Documents/RAGFLOW/ai-tongtong/node_modules/.pnpm/@antfu+eslint-config@4.19.0_@vue+compiler-sfc@3.5.22_eslint@9.36.0_jiti@2.6.0__typescript@5.8.3/node_modules:/Users/Bone/Documents/RAGFLOW/ai-tongtong/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/Bone/Documents/RAGFLOW/ai-tongtong/node_modules/.pnpm/@antfu+eslint-config@4.19.0_@vue+compiler-sfc@3.5.22_eslint@9.36.0_jiti@2.6.0__typescript@5.8.3/node_modules/@antfu/eslint-config/bin/node_modules:/Users/Bone/Documents/RAGFLOW/ai-tongtong/node_modules/.pnpm/@antfu+eslint-config@4.19.0_@vue+compiler-sfc@3.5.22_eslint@9.36.0_jiti@2.6.0__typescript@5.8.3/node_modules/@antfu/eslint-config/node_modules:/Users/Bone/Documents/RAGFLOW/ai-tongtong/node_modules/.pnpm/@antfu+eslint-config@4.19.0_@vue+compiler-sfc@3.5.22_eslint@9.36.0_jiti@2.6.0__typescript@5.8.3/node_modules/@antfu/node_modules:/Users/Bone/Documents/RAGFLOW/ai-tongtong/node_modules/.pnpm/@antfu+eslint-config@4.19.0_@vue+compiler-sfc@3.5.22_eslint@9.36.0_jiti@2.6.0__typescript@5.8.3/node_modules:/Users/Bone/Documents/RAGFLOW/ai-tongtong/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/index.js" "$@"
else
  exec node  "$basedir/../../bin/index.js" "$@"
fi
